<%= @car.inspect %>
<%= form_for @car, url: car_create_path, html: { multipart: true } do |f| %>
	<%= f.label :city_id %>
	<%= f.collection_select :city_id, City.all, :id, :name %>

	<%= f.label :brand_id %>
	<%= f.collection_select :brand_id, Brand.all, :id, :name %>

	<%= f.label :model_id %>
	<%= f.select :model_id, "", {}, disabled: true %>

	<%= f.label :price %>
	<%= f.text_field :price %>

	<%= f.label :transmission %>
	<%= f.select :transmission, ["", "АКПП", "механика", "типтроник"] %>

	<%= f.label :wheel %>
	<%= f.select :wheel, ["левый", "правый"] %>

	<%= f.label :year %>
	<%= f.select :year, 1980..Time.now.year, selected: 2000 %>
	<br/>

	<%= f.fields_for :car_images, @car.car_images.build do |forma| %>
		<%= forma.file_field :image, multiple: true %>
	<% end %>
	<br/><br/>

	<%= f.submit "Добавить авто" %>
<% end %>